{
  "manifest_version": 3,
  "name": "Lamzu Aurora Shortcut",
  "version": "1.0",
  "description": "Opens Lamzu Aurora in a new Chrome tab",
  "action": {
    "default_icon": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  }
}